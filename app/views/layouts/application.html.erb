<!DOCTYPE html>
<html>
  <head>
    <title>Tipoyapo</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
    <nav class="navbar navbar-default">
    <div class="container-fluid">
      <div class="navbar-header">
        <a class="navbar-brand" href="#">Tipoyapo</a>
      </div>
      <ul class="nav navbar-nav">
        <li class="active"><a href="#">Home</a></li>
        <li><a href="<%= new_user_session_path %>">Ingresar</a></li>
        <li><a href="<%= new_user_registration_path %>">Registrarse</a></li>
        <li><a href="<%= destroy_user_session_path %>" data-method="delete">Salir</a></li>
      </ul>
    </div>
  </nav>
    <%= yield %>
  </body>
</html>
